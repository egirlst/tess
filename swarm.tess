f! main {
    print:: "Fetching data from swarm.ws..."
    response = request:: "GET" "https://swarm.ws/"
    
    print:: "Writing response to file..."
    
    $$ Low-level file I/O using 'f' object
    file = f.open("output.json", "w")
    file.write(response)
    file.close()
    
    print:: "done"
}

start >.<
